generateheaders: $(GENERATEDHEADERS)
	for i in $(SUBDIRS); do (cd $$i && $(MAKE) generateheaders) || exit 1; done

clean:
	rm -f *.o *_n.cc *_n.h *_m.cc *_m.h .tstamp
	rm -f *.vec *.sca
	for i in $(SUBDIRS); do (cd $$i && $(MAKE) clean); done

depend:
	$(MAKEDEPEND) $(INCLUDE_PATH) -- *.cc
	# $(MAKEDEPEND) $(INCLUDE_PATH) -fMakefile.in -- *.cc
	for i in $(SUBDIRS); do (cd $$i && $(MAKE) depend) || exit 1; done

makefiles:
	# recreate Makefile
	opp_makemake -f  -N -b /home/administrator/Developement/eclipse/Castalia -c /home/administrator/Developement/eclipse/Castalia/config/Castalia.config -I. -u Cmdenv -n -r -I/home/administrator/Developement/eclipse/Castalia/src/Node/Communication/MAC -I/home/administrator/Developement/eclipse/Castalia/src/Node/Resource_Manager -I/home/administrator/Developement/eclipse/Castalia/src/Wireless_Channel 
	for i in $(SUBDIRS); do (cd $$i && $(MAKE) makefiles) || exit 1; done

makefile-ins:
	# recreate Makefile.in
	opp_makemake -f -m  -N -b /home/administrator/Developement/eclipse/Castalia -c /home/administrator/Developement/eclipse/Castalia/config/Castalia.config -I. -u Cmdenv -n -r -I/home/administrator/Developement/eclipse/Castalia/src/Node/Communication/MAC -I/home/administrator/Developement/eclipse/Castalia/src/Node/Resource_Manager -I/home/administrator/Developement/eclipse/Castalia/src/Wireless_Channel 
	for i in $(SUBDIRS); do (cd $$i && $(MAKE) makefile-ins) || exit 1; done

